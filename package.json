{
  "private": true,
  "name": "jennydaman.github.io",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --service-worker false",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "if-env": "^1.0.0",
    "preact-cli": "^2.1.0"
  },
  "dependencies": {
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "preact-material-components": "^1.3.7"
  }
}
